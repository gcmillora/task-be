{
  "moduleFileExtensions": ["js", "json", "ts"],
   "preset": "ts-jest",
  "rootDir": "./",
  "modulePaths": ["<rootDir>"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
      "^src$": "<rootDir>/src",
      "^src/(.+)$": "<rootDir>/src/$1"
    }
}
